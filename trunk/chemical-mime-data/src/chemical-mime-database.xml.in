<!DOCTYPE chemical-mime [
	<!-- Root database element. -->
	<!ELEMENT chemical-mime (mime-type+)>
	<!ATTLIST chemical-mime
	          id CDATA #REQUIRED>

	<!-- The MIME type (only chemical/*). -->
	<!ELEMENT mime-type (comment+, (acronym, expanded-acronym)*, glob+, root-XML*, sub-class-of*, alias*, magic?, icon?, supported-by?, specification*)>
	<!ATTLIST mime-type
	          type CDATA #REQUIRED
	          support (yes|no) "no"
	          added CDATA #IMPLIED>

	<!-- Comment describing a document with the respective MIME type. -->
	<!ELEMENT comment (#PCDATA)>
	<!ATTLIST comment
	          xml:lang CDATA #IMPLIED>

	<!-- Comment describing the respective unexpanded MIME type acronym. -->
	<!ELEMENT acronym (#PCDATA)>
	<!ATTLIST acronym
	          xml:lang CDATA #IMPLIED>

	<!-- Comment describing the respective unexpanded MIME type acronym. -->
	<!ELEMENT expanded-acronym (#PCDATA)>
	<!ATTLIST expanded-acronym
	          xml:lang CDATA #IMPLIED>

	<!-- File pattern of the MIME type. -->
	<!ELEMENT glob (conflicts)*>
	<!ATTLIST glob
            pattern CDATA #REQUIRED>

	<!-- Conflicting MIME-type(s). Use the @gnome and/or @kde attributes to 
	     determine if we will have problems with KDE 3.x, GNOME <= 2.4 or both. -->
	<!ELEMENT conflicts EMPTY>
	<!ATTLIST conflicts
	          type CDATA #REQUIRED
	          cmd (yes|no) "no"
	          fdo (yes|no) "no"
	          gnome (yes|no) "no"
	          kde (yes|no) "no">

	<!-- The root XML element and the namespace. -->
	<!ELEMENT root-XML EMPTY>
	<!ATTLIST root-XML
	          namespaceURI CDATA #REQUIRED
	          localName CDATA #REQUIRED>

	<!-- The main class the MIME type belongs to. Mainly text/plain or text/xml. -->
	<!ELEMENT sub-class-of EMPTY>
	<!ATTLIST sub-class-of
	          type CDATA #REQUIRED>

	<!-- The alias of the specified MIME type if any exists. -->
	<!ELEMENT alias EMPTY>
	<!ATTLIST alias
	          type CDATA #REQUIRED>

	<!-- The magic pattern. Refer to the shared-mime-info database specification.
	     Search http://sourceforge.net/docman/?group_id=159685 for the link. -->
	<!ELEMENT magic (match)+>
	<!ATTLIST magic
	          priority CDATA #IMPLIED>
	<!ELEMENT match (match)*>
	<!ATTLIST match
	          offset CDATA #REQUIRED
	          type (string|big16|big32|little16|little32|host16|host32|byte) #REQUIRED
	          value CDATA #REQUIRED
	          mask CDATA #IMPLIED>

	<!-- Icon for GNOME 2.4 and/or KDE 3.x system. Use the @gnome and/or @kde
	     attributes to determine the icon especially for KDE and/or GNOME. -->
	<!ELEMENT icon EMPTY>
	<!ATTLIST icon
	          gnome CDATA #IMPLIED
	          kde CDATA #IMPLIED>

	<!-- List of applications supporting the respective MIME type.
	     Use the @read and @write attributes to determine, if the application
	     reads or writes or reads&writes the specified MIME type -->
	<!ELEMENT supported-by (application+)>
	<!ELEMENT application EMPTY>
	<!ATTLIST application
	          name CDATA #REQUIRED
	          read CDATA #IMPLIED
	          write CDATA #IMPLIED
	          package CDATA #IMPLIED>

	<!-- URL where to find the MIME/file type specification or explanation. -->
	<!ELEMENT specification EMPTY>
	<!ATTLIST specification
	          url CDATA #REQUIRED
	          title CDATA #IMPLIED>
]>

<chemical-mime id="'$Id$'">

	<mime-type type="application/x-chemtool" support="no">
		<_comment>Chemtool File</_comment>
		<glob pattern="*.cht"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value="Chemtool Version " offset="0"/>
		</magic>
		<icon kde="chemtool"/>
		<supported-by>
			<application name="chemtool"/>
		</supported-by>
	</mime-type>

	<mime-type type="application/x-ghemical" support="no">
		<_comment>Ghemical QM Format</_comment>
		<acronym>QM</acronym>
		<expanded-acronym>Quantum Mechanics</expanded-acronym>
		<glob pattern="*.gpr"/>
		<glob pattern="*.mm1gp"/>
		<glob pattern="*.qm1gp"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value="!Header gpr " offset="0">
				<match type="string" value="100\n" offset="12"/>
				<match type="string" value="110\n" offset="12"/>
				<match type="string" value="111\n" offset="12"/>
			</match>
			<match type="string" value="!Header mmlgp " offset="0">
				<match type="string" value="100\n" offset="14"/>
			</match>
		</magic>
		<supported-by>
			<application name="ghemical"/>
		</supported-by>
	</mime-type>

	<mime-type type="application/x-xdrawchem" support="no">
		<_comment>Native XDrawChem Format</_comment>
		<glob pattern="*.xdc"/>
		<!-- <root-XML namespaceURI="http://www.xdrawchem.org" localName="xdrawchem"/> -->
		<sub-class-of type="application/xml"/>
		<magic priority="50">
			<match type="string" value="&lt;!DOCTYPE xdrawchem" offset="0:256"/>
			<match type="string" value="&lt;xdrawchem" offset="0:64"/>
		</magic>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/seq-aa-fasta" support="no">
		<_comment>Fasta Amino Acid Sequence</_comment>
		<glob pattern="*.aa"/>
		<!-- see also chemical/x-fasta -->
		<sub-class-of type="chemical/x-fasta"/>
		<specification url="http://msdlocal.ebi.ac.uk/docs/mimetype.html"/>
	</mime-type>

	<mime-type type="chemical/seq-na-fasta" support="no">
		<_comment>Fasta Nucleotide Sequence</_comment>
		<glob pattern="*.na"/>
		<!-- see also chemical/x-fasta -->
		<sub-class-of type="chemical/x-fasta"/>
		<specification url="http://msdlocal.ebi.ac.uk/docs/mimetype.html"/>
	</mime-type>

	<mime-type type="chemical/seq-aa-genpept" support="no">
		<_comment>GenPept Flat File</_comment>
		<glob pattern="*.gp">
			<conflicts type="application/x-gnuplot" fdo="yes"/>
		</glob>
		<sub-class-of type="text/plain"/>
		<specification url="http://msdlocal.ebi.ac.uk/docs/mimetype.html"/>
	</mime-type>

	<mime-type type="chemical/x-alchemy" support="yes" added="0.1.91">
		<_comment>Alchemy Format</_comment>
		<glob pattern="*.alc"/>
		<sub-class-of type="text/plain"/>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.camsoft.com/plugins/"/>
	</mime-type>

	<mime-type type="chemical/x-cache" support="yes" added="0.1.91">
		<_comment>CAChe MolStruct Format</_comment>
		<acronym>CAChe</acronym>
		<expanded-acronym>Computer Aided Chemistry</expanded-acronym>
		<glob pattern="*.cac"/>
		<glob pattern="*.cache"/>
		<sub-class-of type="text/plain"/>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-cache-csf" support="no">
		<_comment>CAChe CSF Format</_comment>
		<acronym>CAChe</acronym>
		<expanded-acronym>Computer Aided Chemistry</expanded-acronym>
		<glob pattern="*.csf"/>
		<sub-class-of type="text/plain"/>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-cactvs-ascii" support="yes" added="0.1.94">
		<_comment>CACTVS ASCII Format</_comment>
		<acronym>ASCII</acronym>
		<expanded-acronym>American Standard Code for Information Interchange</expanded-acronym>
		<acronym>CACTVS</acronym>
		<expanded-acronym>Chemical Algorithms Construction, Threading and Verification System</expanded-acronym>
		<glob pattern="*.cascii"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value="#" offset="0">
				<match type="string" value="Cactvs NMDSAscii by" offset="0:100"/>
			</match>
		</magic>
		<supported-by>
			<application name="csbr (cactvs)"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-cactvs-binary" support="yes" added="0.1.94">
		<_comment>CACTVS Binary Format</_comment>
		<acronym>CACTVS</acronym>
		<expanded-acronym>Chemical Algorithms Construction, Threading and Verification System</expanded-acronym>
		<glob pattern="*.cbin"/>
		<!-- Byte 0:7 -> 8 Byte header by XDR? -->
		<!-- Byte 0:7 -> Can it be specified?  -->
		<magic priority="50">
			<match type="byte" value="07" offset="8">
				<match type="string" value="CACTVSBIN" offset="9"/>
			</match>
		</magic>
		<supported-by>
			<application name="csbr (cactvs)"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-cactvs-table" support="yes" added="0.1.94">
		<_comment>CACTVS Table Format</_comment>
		<acronym>CACTVS</acronym>
		<expanded-acronym>Chemical Algorithms Construction, Threading and Verification System</expanded-acronym>
		<glob pattern="*.ctab"/>
		<magic priority="50">
			<match type="string" value="CACTVS QSAR Table" offset="0"/>
		</magic>
		<supported-by>
			<application name="csbr (cactvs)"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-cdx" support="yes" added="0.1.91">
		<_comment>ChemDraw Binary CDX Format</_comment>
		<acronym>CDX</acronym>
		<expanded-acronym>ChemDraw eXchange</expanded-acronym>
		<glob pattern="*.cdx"/>
		<magic priority="50">
			<match type="string" value="VjCD0100" offset="0">
				<match type="little32" value="0x01020304" offset="8">
					<match type="little32" value="0x00000000" offset="12">
						<match type="little32" value="0x00000000" offset="16">
							<match type="little32" value="0x80000000" offset="20"/>
							<match type="little32" value="0x00000000" offset="20"/>
						</match>
					</match>
				</match>
			</match>
		</magic>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.camsoft.com/plugins/"/>
		<specification url="http://www.cambridgesoft.com/services/documentation/sdk/chemdraw/cdx/index.htm"/>
		<specification url="http://www.cambridgesoft.com/services/documentation/sdk/chemdraw/cdx/IntroCDX.htm"/>
	</mime-type>

	<!-- probably not official -->
	<mime-type type="chemical/x-cdxml" support="yes" added="0.1.94">
		<_comment>ChemDraw CDXML Format</_comment>
		<acronym>CDXML</acronym>
		<expanded-acronym>ChemDraw eXchange XML</expanded-acronym>
		<glob pattern="*.cdxml"/>
		<!-- <root-XML namespaceURI="http://www.camsoft.com" localName="CDXML"/> -->
		<sub-class-of type="application/xml"/>
		<magic priority="50">
			<match type="string" value="&lt;!DOCTYPE CDXML" offset="0:256"/>
			<match type="string" value="&lt;CDXML" offset="0:64"/>
		</magic>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.camsoft.com/plugins/"/>
		<specification url="http://www.cambridgesoft.com/services/documentation/sdk/chemdraw/cdx/index.htm"/>
		<specification url="http://www.cambridgesoft.com/services/documentation/sdk/chemdraw/cdx/IntroCDXML.htm"/>
	</mime-type>

	<mime-type type="chemical/x-cerius" support="no">
		<_comment>MSI Cerius II format</_comment>
		<acronym>MSI</acronym>
		<expanded-acronym>Molecular Simulations Inc.</expanded-acronym>
		<glob pattern="*.cer">
			<conflicts type="application/x-x509-ca-cert" fdo="yes" kde="yes"/>
		</glob>
		<sub-class-of type="text/plain"/>
	</mime-type>

	<mime-type type="chemical/x-chemdraw" support="no">
		<_comment>ChemDraw File</_comment>
		<glob pattern="*.chm">
			<conflicts type="application/chm" kde="yes"/>
			<conflicts type="application/x-chm" fdo="yes" gnome="yes"/>
		</glob>
		<alias type="application/x-chemdraw"/>
	</mime-type>

	<mime-type type="chemical/x-chem3d" support="yes" added="0.1.91">
		<_comment>Chem3D Format</_comment>
		<glob pattern="*.c3d"/>
		<sub-class-of type="text/plain"/>
		<supported-by>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.camsoft.com/plugins/"/>
	</mime-type>

	<mime-type type="chemical/x-cif" support="yes" added="0.1.91">
		<_comment>Crystallographic Information File Format</_comment>
		<acronym>CIF</acronym>
		<expanded-acronym>Crystallographic Information File</expanded-acronym>
		<glob pattern="*.cif"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value="#\#CIF_1.1" offset="0">
				<match type="byte" value="9" offset="10"/>
				<match type="byte" value="10" offset="10"/>
				<match type="byte" value="13" offset="10"/>
			</match>
		</magic>
		<supported-by>
			<application name="gdis"/>
			<application name="jmol"/>
		</supported-by>
		<specification url="http://www.iucr.org/iucr-top/cif/spec/"/>
	</mime-type>

	<mime-type type="chemical/x-cmdf" support="no">
		<_comment>CrystalMaker Crystal Binary File</_comment>
		<glob pattern="*.cmdf"/>
		<glob pattern="*.cmd5"/><!-- old format -->
		<magic priority="50">
			<match type="string" value="CMDFCrystalMakerM" offset="0"/>
			<match type="string" value="CMD5(CrystalMaker)" offset="0"/>
		</magic>
		<specification url="http://www.crystalmaker.com/library/index.html"/>
	</mime-type>

	<mime-type type="chemical/x-cml" support="yes" added="0.1.90">
		<_comment>Chemical Markup Language</_comment>
		<acronym>CML</acronym>
		<expanded-acronym>Chemical Markup Language</expanded-acronym>
		<glob pattern="*.cml"/>
		<root-XML namespaceURI="http://www.xml-cml.org/schema" localName="cml"/>
		<root-XML namespaceURI="http://www.xml-cml.org/schema/cml2/core" localName="cml"/>
		<sub-class-of type="text/xml"/>
		<alias type="chemical/cml"/>
		<magic priority="50">
			<match type="string" value="&lt;!DOCTYPE cml" offset="0:256"/>
			<match type="string" value="&lt;!DOCTYPE molecule" offset="0:256"/>
			<match type="string" value="&lt;cml" offset="0:64"/>
			<match type="string" value="&lt;molecule" offset="0:64"/>
		</magic>
		<supported-by>
			<application name="gchempaint"/>
			<application name="jmol"/>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.xml-cml.org/"/>
	</mime-type>

	<mime-type type="chemical/x-cmmf" support="no">
		<_comment>CrystalMaker Molecule Binary File</_comment>
		<glob pattern="*.cmmf"/>
		<glob pattern="*.cmm5"/><!-- old format -->
		<magic priority="50">
			<match type="string" value="CMMFCrystalMakerM" offset="0"/>
			<match type="string" value="CMM5(CrystalMaker)" offset="0"/>
		</magic>
		<specification url="http://www.crystalmaker.com/library/proteins.html"/>
	</mime-type>

	<mime-type type="chemical/x-cmtx" support="no">
		<_comment>CrystalMaker Crystal/Molecule Text File</_comment>
		<glob pattern="*.cmtx"/>
		<magic priority="80">
			<match type="string" value="TITL" offset="0">
				<match type="string" value="NOTE" offset="5:81"/>
			</match>
			<!-- molecule files begin with ^MOLE -->
			<match type="string" value="MOLE" offset="0">
				<match type="string" value="TITL" offset="5:81">
					<match type="string" value="NOTE" offset="10:161"/>
				</match>
			</match>
		</magic>
		<specification url="http://www.crystalmaker.co.uk/downloads/CM_Users_Guide.pdf"/>
	</mime-type>

	<mime-type type="chemical/x-compass" support="no">
		<_comment>COMPASS Program of the Takahashi</_comment>
		<glob pattern="*.cpa"/>
	</mime-type>

	<mime-type type="chemical/x-crossfire" support="no">
		<_comment>CrossFire File</_comment>
		<glob pattern="*.bsd"/>
	</mime-type>

	<mime-type type="chemical/x-csml" support="no">
		<_comment>Chemical Style Markup Language</_comment>
		<acronym>CSML</acronym>
		<expanded-acronym>Chemical Style Markup Language</expanded-acronym>
		<glob pattern="*.csm"/>
		<glob pattern="*.csml"/>
	</mime-type>

	<mime-type type="chemical/x-ctx" support="no">
		<_comment>Gasteiger ClearText File Format</_comment>
		<glob pattern="*.ctx"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value=" /IDENT        " offset="0"/>
		</magic>
	</mime-type>

	<mime-type type="chemical/x-cxf" support="no">
		<_comment>Chemical eXchange Format</_comment>
		<acronym>CXF</acronym>
		<expanded-acronym>Chemical eXchange Format</expanded-acronym>
		<!-- <glob pattern="*.cef"/> -->
		<glob pattern="*.cxf"/>
	</mime-type>

	<mime-type type="chemical/x-daylight-smiles" support="yes" added="0.1.91">
		<_comment>SMILES Format</_comment>
		<acronym>SMILES</acronym>
		<expanded-acronym>Simplified Molecular Input Line Entry Specification</expanded-acronym>
		<glob pattern="*.smi">
			<conflicts type="application/smil" fdo="yes" kde="yes"/>
			<conflicts type="application/x-smil" gnome="yes"/>
		</glob>
		<glob pattern="*.smiles"/>
		<sub-class-of type="text/plain"/>
		<supported-by>
			<application name="csbr (cactvs)"/>
			<application name="xdrawchem"/>
		</supported-by>
		<specification url="http://www.daylight.com/smiles/"/>
	</mime-type>

	<mime-type type="chemical/x-dmol" support="yes" added="0.1.91">
		<_comment>DMol Output Format</_comment>
		<glob pattern="*.dmol"/>
		<glob pattern="*.outmol"/>
		<sub-class-of type="text/plain"/>
		<icon kde="qc"/>
		<supported-by>
			<application name="viewmol"/>
		</supported-by>
	</mime-type>

	<mime-type type="chemical/x-embl-dl-nucleotide" support="no">
		<_comment>EMBL Nucleotide Format</_comment>
		<acronym>EMBL</acronym>
		<expanded-acronym>European Molecular Biology Laboratory</expanded-acronym>
		<glob pattern="*.emb"/>
		<magic priority="50">
			<match type="string" value="ID   " offset="0"/>
		</magic>
		<specification url="http://mercury.ebi.ac.uk/"/>
		<specification url="http://www.ebi.ac.uk/embl/Documentation/User_manual/usrman.html#FORMAT"/>
		<specification url="http://www.biobind.com/faq/emboss/formats.html"/>
		<specification url="http://www.psc.edu/general/software/packages/seq-intro/emblfile.html"/>
	</mime-type>

	<mime-type type="chemical/x-fasta" support="no">
		<_comment>Fasta File Format</_comment>
		<glob pattern="*.fa"/>
		<glob pattern="*.fsa"/>
		<sub-class-of type="text/plain"/>
		<magic priority="50">
			<match type="string" value="&lt;" offset="0"/>
		</magic>
		<specification url="http://www.ncbi.nlm.nih.gov/blast/fasta.shtml"/>
	</mime-type>

	<mime-type type="chemical/x-galactic-spc" support="no">
		<_comment>SPC Format for Spectral and Chromatographic Data</_comment>
		<!-- <glob pattern="*.fir"/> -->
		<!-- <glob pattern="*.rmn"/> -->
		<glob pattern="*.spc"/>
		<specification url="https://ftirsearch.com/features/converters/SPCFileFormat.htm"/>
		<specification url="http://web.archive.org/web/20021201230302/www.galactic.com/instruments/spc.htm"/>
	</mime-type>

	<mime-type type="chemical/x-gamess-input" support="yes" added="0.1.93">
		<_comment>GAMESS Input format</_comment>
		<acronym>GAMESS</acronym>
		<expanded-acronym>General Atomic and Molecular Electronic Structure System</expanded-acronym>
		<glob pattern="*.gamin"/>
		<glob pattern="*.inp"/>
		<sub-class-of type="text/plain"/>
		<magic priority="80">
			<!-- These patterns will not hit files with large -->
			<!-- comments at their beginning.                 -->
			<match type="string" value="$CONTRL" offset="0:80">
				<match type="string" value="AIMPAC" offset="8:80"/>
				<match type="string" value="CCTYP" offset="8:80"/>
				<match type="string" value="CITYP" offset="8:80"/>
				<match type="string" value="COORD" offset="8:80"/>
				<match type="string" value="DFTTYP" offset="8:80"/>
				<match type="string" value="EXETYP" offset="8:80"/>
				<match type="string" value="FRIEND" offset="8:80"/>
				<match type="string" value="GEOM" offset="8:80"/>
				<match type="string" value="GRDTYP" offset="8:80"/>
				<match type="string" value="ICHARG" offset="8:80"/>
				<match type="string" value="ICUT" offset="8:80"/>
				<match type="string" value="INTTYP" offset="8:80"/>
				<match type="string" value="ISPHER" offset="8:80"/>
				<match type="string" value="ITOL" offset="8:80"/>
				<match type="string" value="LOCAL" offset="8:80"/>
				<match type="string" value="MAXIT" offset="8:80"/>
				<match type="string" value="MOLPLT" offset="8:80"/>
				<match type="string" value="MPLEVEL" offset="8:80"/>
				<match type="string" value="MULT" offset="8:80"/>
				<match type="string" value="NPRINT" offset="8:80"/>
				<match type="string" value="NORMF" offset="8:80"/>
				<match type="string" value="NORMP" offset="8:80"/>
				<match type="string" value="NOSYM" offset="8:80"/>
				<match type="string" value="NUMGRD" offset="8:80"/>
				<match type="string" value="NZVAR" offset="8:80"/>
				<match type="string" value="PLTORB" offset="8:80"/>
				<match type="string" value="PP" offset="8:80"/>
				<match type="string" value="QMTTOL" offset="8:80"/>
				<match type="string" value="RELWFN" offset="8:80"/>
				<match type="string" value="RUNTYP" offset="8:80"/>
				<match type="string" value="SCFTYP" offset="8:80"/>
				<match type="string" value="TDDFT" offset="8:80"/>
				<match type="string" value="TREST" offset="8:80"/>
